<Window x:Class="ProjectJam.AssetCompletedMaintenance"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectJam"
        mc:Ignorable="d"
        Title="AssetCompletedMaintenance" Height="300" Width="300">
    <Grid>
        <Label x:Name="completeMaintenanceTitleLabel" Content="Complete maintenance" HorizontalAlignment="Left" Margin="62,10,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
        <Label x:Name="assetLabel" Content="Asset:" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="completeMaintenanceComboBox" HorizontalAlignment="Left" Margin="62,60,0,0" VerticalAlignment="Top" Width="120"/>
        <ListBox x:Name="completedMaintenanceListBox" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="120" Margin="62,95,0,0"/>
        <Button x:Name="completeButton" Content="Complete" HorizontalAlignment="Left" Margin="207,220,0,0" VerticalAlignment="Top" Width="75" Click="completeButton_Click"/>
        <Button x:Name="backButton" Content="Back" HorizontalAlignment="Left" Margin="10,220,0,0" VerticalAlignment="Top" Width="55" Click="backButton_Click"/>

    </Grid>
</Window>
